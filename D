import requests
import telebot
import random
import os
from telebot import types
from time import sleep 
print("""Ù…Ø·ÙˆØ± Ø§Ù„Ø¨ÙˆØª > @kkkkkgk 
Ù‚Ù†Ø§Ø© Ù…Ø·ÙˆØ± Ø§Ù„Ø¨ÙˆØª  > @h8ppp 
Ù…Ù„Ø§Ø­Ø¸Ø©:Ø§Ù„Ø¨ÙˆØª ÙƒØ§Ù† Ø¨Ù‡ Ø¨Ø¹Ø¶ Ø§Ù„Ø§Ø®Ø·Ø§Ø¡ ØªÙ… ØªØ¹Ø¯ÙŠÙ„ Ø§Ù„Ø§Ø®Ø·Ø§Ø¡ 
Ø¨ÙˆØ§Ø³Ø·Ø© > @y_j_e
""")
sleep(3)
os.system('clear')
mar = '1234567890'
bot = telebot.TeleBot('1977591036:AAFk-Rq_7dHjR5iX6HGTQDN47ogkbLgsMGU')
call  = types.InlineKeyboardButton(text = "START", callback_data = 'sssk')
@bot.message_handler(commands=['start'])
def start(message):
    Keyy = types.InlineKeyboardMarkup()
    Keyy.row_width = 1
    Keyy.add(call)
    bot.send_message(message.chat.id, text=f"""* - Ø§Ù„Ø§Ø¯Ø§Ø© ØªÙØ­Øµ Ø¨ÙŠÙ†Ø§Øª :
    - Ø£Ù…Ø±ÙŠÙƒÙŠ *
    Ø³ÙŠØªÙ… Ø§Ø¶Ø§ÙØ© Ø¯ÙˆÙ„ Ø§Ø®Ø±Ù‰ Ù‚Ø±ÙŠØ¨Ø§""",parse_mode="markdown",reply_markup=Keyy)
@bot.callback_query_handler(func=lambda call: True)
def answer(call):
    if call.data =="sssk":
        button(call.message)
    if call.data =="br":
        BIN(call.message)
    if call.data =="im":
         IM(call.message)


lc = types.InlineKeyboardButton(text = "Ø§Ø¶ØºØ· Ù‡Ù†Ø§", callback_data = 'im')
def button(message):
    ms = types.InlineKeyboardMarkup(row_width=1)
    ms.add(lc)
    bot.edit_message_text(chat_id=message.chat.id,message_id=message.message_id,text="**Ù‡Ù„ ØªØ±ÙŠØ¯ Ø¨Ø¯Ø¡ Ø§Ù„ÙØ­ØµØŸ**",parse_mode='markdown',reply_markup=ms)        


def BIN(message):
        l = 0
        i = 0
        mar = '0123456789'  
        while True:
            
            bin = str(''.join((random.choice(mar) for i in range(5))))
            rq = requests.post ("https://lookup.binlist.net/4"+bin)
        
            if '"scheme":"amex"' in rq.text:
                if '"type":"credit"' in rq.text:
                    if '"America"' in rq.text:
                        i+=1
                        bin_trrue = f"""
                        bin : 4{bin} True âœ…                  
                        """
                        bot.send_message(message.chat.id, text=bin_trrue)
            else:
                l+=1
            mees = types.InlineKeyboardMarkup(row_width=1)
            buut = types.InlineKeyboardButton(f"ğŸ’µâ ØªÙ… ÙØ­Øµ  : {l} Ø¨ÙŠÙ† ",callback_data='jhi')
            buut5 = types.InlineKeyboardButton(f"âš ï¸ In : 4{bin}",callback_data='jhi5')
            buut1 = types.InlineKeyboardButton(f"âœ…â hits : {i}",callback_data='jhi1')            
            mees.add(buut,buut1,buut5)
            bot.edit_message_text(chat_id=message.chat.id,message_id=message.message_id,text="**Cheaker Bins**",parse_mode='markdown',reply_markup=mees)        
                
        mees = types.InlineKeyboardMarkup(row_width=1)
        buut = types.InlineKeyboardButton(f"ğŸ’µâ bin good : {k}",callback_data='jhi')
        buut5 = types.InlineKeyboardButton(f"âš ï¸ : 37{bin}",callback_data='jhi5')
        buut1 = types.InlineKeyboardButton(f"âœ…â hits : {h}",callback_data='jhi1')            
        mees.add(buut,buut1,buut5)
        bot.edit_message_text(chat_id=message.chat.id,message_id=message.message_id,text="**Cheaker Bins**",parse_mode='markdown',reply_markup=mees)
        
