            buut5 = types.InlineKeyboardButton(f"âš ï¸ ØªÙ… ÙØ­Øµ Ù‡Ø°Ø§ Ø§Ù„Ø¨ÙŠÙ† : 4{bin}",callback_data='jhi5')
            buut1 = types.InlineKeyboardButton(f"âœ…â Ø¨ÙŠÙ†Ø§Øª Ø´ØºØ§Ù„Ø© : {i}",callback_data='jhi1')            
            mees.add(buut,buut1,buut5)
